
<header>
  <nav class="navbar navbar-dark">

    <div>
      <img src="/docs/l.png" alt="L" class="d-inline-block">
      <a class="navbar-brand" id="logo-text" href="/">OGO HERE</a>
      <a class="messages link" href="/messages"></a>
    </div>
    <a id="main-title" href="/">THE Market Place</a>

      <% if(user){ %>
    <div class="user-div">
      <form class="user-name" method="POST" action="/logout">
        <button class="btn btn-warning" type="button" onclick="location.href='/favourites'">Your Favourites</button>
        <a href="/user">Logged in as: <%= user%></a>
        <div>
         
          <button class="btn btn-light">Logout</button>
        </div>
      </form>

      <% } else{ %>
        <div>
          <a class="nav-item" href="/register">Register</a>
          <a class="nav-item" href="/login">Login</a>
        </div>
    </div>
    <% } %>
  </nav>
</header>
